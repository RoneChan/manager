<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.TestRuleMapper">

    <!--根据交易名查询所有有效的测试规则-->
    <select id="getTestRuleByTradeName" resultType="com.example.demo.dto.TestRuleManage">
        SELECT * FROM testrulemanage WHERE tradeName=#{tradeNsme};
    </select>

    <!--查询所有测试规则-->
    <select id="getTestRules" resultType="com.example.demo.dto.TestRuleManage">
        SELECT * FROM testrulemanage;
    </select>

    <!--添加测试规则-->

</mapper>